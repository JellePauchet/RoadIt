@model RoadIt.Models.Truck

@{
    ViewBag.Title = "Transporter report";
}

<h2>Transporter report</h2>
@{
    if (Session["RoleId"].ToString() == "3")
    {
        <p>The next data that is showed is for roadsection: @Session["RoadID"].ToString()</p>
        @Html.Raw(ViewData["Truck"].ToString())
        @Html.Raw(ViewData["Compactor"].ToString())
    }
    else
    {
        ViewBag.error = "You are not authorized to view this page";
        @ViewBag.error
    } 
} 