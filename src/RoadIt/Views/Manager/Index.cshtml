@model RoadIt.Models.UA

@{
    ViewBag.Title = "ManagerReport";
}

<h2>Report</h2>

@{
    if(Session["RoadId"].ToString() != "")
    {
        <p>The next data that is showed is for roadsection: @Session["RoadID"].ToString()</p>
        @Html.Raw(ViewData["tableSpec"].ToString())
        @Html.Raw(ViewData["tableTransport"].ToString())
        @Html.Raw(ViewData["tableTotal"].ToString())
        @Html.Raw(ViewData["tablePlant"].ToString())
        @Html.Raw(ViewData["tableMix"].ToString())
        <div class="btn-group">
            <button type="button" class="btn btn-default" id="cmd">generate PDF</button>
            <button class="btn btn-default" onclick="myFunction()">Print this page</button>
        </div>
    }
    else
    {
        ViewBag.error = "First choose a roadsection";
        @ViewBag.error
    } 
}
